{
  "name": "homebridge-pc-volume",
  "version": "1.1.0",
  "description": "Control your system's volume using Apple's HomeKit",
  "author": "Joseph Duffy",
  "license": "MIT",
  "main": "build/index.js",
  "preferGlobal": true,
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "build:watch": "nodemon --watch src/index.ts --watch tsconfig.json --watch package.json --exec \"tsc && homebridge --debug\"",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "@types/node": "^10.12.12",
    "loudness": "^0.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.8",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": ">=4.4.1",
    "homebridge": ">=0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JosephDuffy/homebridge-pc-volume.git"
  },
  "keywords": [
    "homebridge-plugin",
    "volume",
    "mac",
    "linux"
  ]
}
